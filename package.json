  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.2",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "webpack": "3.8.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
